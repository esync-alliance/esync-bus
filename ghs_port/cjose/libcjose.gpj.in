#!gbuild
macro __LIB_JANSSON_DIR=@LIB_JANSSON_DIR@
macro __LIB_OPENSSL_DIR=@LIB_OPENSSL_DIR@
macro __LIB_CJOSE_DIR=@LIB_CJOSE_DIR@
macro __LIB_JANSSON_BUILD_DIR=@LIB_JANSSON_BUILD_DIR@

[Library]
	-object_dir=objs/cjose
        -c99 -g
	-I$__LIB_CJOSE_DIR/include
	-I$__LIB_OPENSSL_DIR/include
	-I$__LIB_JANSSON_DIR/src
        -I$__LIB_JANSSON_BUILD_DIR
        -Dssize_t=long
        -D_SSIZE_T_ALREADY

$__LIB_CJOSE_DIR/src/base64.c
$__LIB_CJOSE_DIR/src/concatkdf.c
$__LIB_CJOSE_DIR/src/error.c
$__LIB_CJOSE_DIR/src/header.c
$__LIB_CJOSE_DIR/src/jwe.c
$__LIB_CJOSE_DIR/src/jwk.c
$__LIB_CJOSE_DIR/src/jws.c
$__LIB_CJOSE_DIR/src/util.c
$__LIB_CJOSE_DIR/src/version.c
